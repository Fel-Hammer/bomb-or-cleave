{
  "name": "bomb-or-cleave",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.20.0",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "build": "tsx other/generate-env.ts && vite build",
    "dev": "vite",
    "lint": "tsgolint --tsconfig tsconfig.app.json && tsgolint --tsconfig tsconfig.node.json",
    "prepare": "simple-git-hooks",
    "preview": "vite preview",
    "test": "vitest --passWithNoTests",
    "typecheck": "tsr generate && tsgo"
  },
  "dependencies": {
    "@tanstack/react-router": "1.134.9",
    "@tanstack/react-router-devtools": "1.134.9",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "1.51.0",
    "@prettier/plugin-oxc": "0.0.4",
    "@tailwindcss/vite": "4.1.16",
    "@tanstack/router-cli": "1.134.9",
    "@tanstack/router-plugin": "1.134.9",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/node": "24.9.2",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@typescript/native-preview": "7.0.0-dev.20251102.1",
    "@vitejs/plugin-react": "5.1.0",
    "lint-staged": "16.2.6",
    "oxlint-tsgolint": "0.4.0",
    "prettier": "3.6.2",
    "prettier-plugin-pkg": "0.21.2",
    "simple-git-hooks": "2.13.1",
    "tailwindcss": "4.1.16",
    "tsx": "4.20.6",
    "vite": "npm:rolldown-vite@7.1.20",
    "vitest": "4.0.6"
  },
  "sideEffects": false,
  "prettier": {
    "plugins": [
      "@prettier/plugin-oxc",
      "prettier-plugin-pkg"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  }
}
